//Run Time: 52ms - 63.56%
//Memory Usage: 23.3MB - 66.55%
//Solved =)
//Time O(n) Memory O(1)
class Solution {
public:
    void reverseString(vector<char>& s) {
        int l = 0;
        int r = s.size() - 1;
        while(l < r) {
            swap(s[l], s[r]);
            ++l;
            --r;
        }
    }
};
