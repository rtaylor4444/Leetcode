//Run Time: 32ms - 85.75%
//Memory Usage: 5.9MB - 64.69%
//Solved =)
//Time O(n) Memory O(1)

class Solution {
public:
    int minPartitions(string n) {
        int maxDigit = 0;
        for(int i = 0; i < n.size(); ++i) {
            maxDigit = max(maxDigit, n[i] - '0');
        }
        return maxDigit;
    }
};
