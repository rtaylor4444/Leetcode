//Run Time: 0ms - 100.00%
//Memory Usage: 5.9MB - 87.48%
//Solved =)
//Time O(n) Memory O(1)
class Solution {
public:
    int xorOperation(int n, int start) {
        int XORResult = 0;
        for(int i = 0; i < n; ++i) {
            XORResult ^= start + (2*i);
        }
        return XORResult;
    }
};
